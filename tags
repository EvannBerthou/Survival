%.o	Makefile	/^%.o: %.cpp$/;"	t
CXXFLAGS	Makefile	/^CXXFLAGS=-Wall -Wextra -g $/;"	m
Game	includes/game.h	/^struct Game {$/;"	s
LIBS	Makefile	/^LIBS=$(shell pkg-config --libs $(PKGS)) -lm$/;"	m
OBJS	Makefile	/^OBJS=$(addsuffix .o, $(basename $(SRCS)))$/;"	m
PKGS	Makefile	/^PKGS=sdl2 SDL2_ttf SDL2_image$/;"	m
SRCS	Makefile	/^SRCS=$(wildcard src\/*.cpp)$/;"	m
all	Makefile	/^all: survival$/;"	t
clean	Makefile	/^clean:$/;"	t
close	src/game.cpp	/^void Game::close() {$/;"	f	class:Game	typeref:typename:void
event	src/game.cpp	/^void Game::event(SDL_Event *event) {$/;"	f	class:Game	typeref:typename:void
init	src/game.cpp	/^void Game::init() {$/;"	f	class:Game	typeref:typename:void
main	src/main.cpp	/^int main() {$/;"	f	typeref:typename:int
render	src/game.cpp	/^void Game::render() {$/;"	f	class:Game	typeref:typename:void
renderer	includes/game.h	/^    SDL_Renderer *renderer;$/;"	m	struct:Game	typeref:typename:SDL_Renderer *
run	src/game.cpp	/^void Game::run() {$/;"	f	class:Game	typeref:typename:void
running	includes/game.h	/^    bool running = true;$/;"	m	struct:Game	typeref:typename:bool
sec	includes/utils.h	/^inline T *sec(T *ptr, const char *msg) {$/;"	f	typeref:typename:T *
sec	includes/utils.h	/^inline int sec(int code, const char *msg) {$/;"	f	typeref:typename:int
survival	Makefile	/^survival: $(OBJS)$/;"	t
update	src/game.cpp	/^void Game::update() {$/;"	f	class:Game	typeref:typename:void
window	includes/game.h	/^    SDL_Window *window;$/;"	m	struct:Game	typeref:typename:SDL_Window *
